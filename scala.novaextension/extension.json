{
  "identifier": "com.brandenvennes.scala",
  "name": "Scala",
  "organization": "Branden Vennes",
  "description": "Scala syntax highlighting, completions,",
  "version": "1.0",
  "categories": [
    "languages",
    "completions"
  ],
  "main": "main.js",
  "activationEvents": [
    "onLanguage:scala"
  ],
  "entitlements": {
    "process": true,
    "filesystem": "readwrite",
    "requests": true
  },
  "commands": {
    "command-palette": [
      {
        "title": "Metals: Import Build",
        "command": "importBuild",
        "filters": {
            "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Update",
        "command": "updateMetals",
        "filters": {
            "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Info",
        "command": "metalsInfo",
        "filters": {
            "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Doctor",
        "command": "openDoctor",
        "fields": {
          "syntaxes": ["scala"]
        }
      }
    ]
  }
}
