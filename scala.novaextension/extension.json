{
  "identifier": "com.brandenvennes.scala",
  "name": "Scala",
  "organization": "com.brandenvennes",
  "description": "Scala language support",
  "version": "0.1.0",
  "homepage": "https://github.com/b-vennes/nova-scala",
  "bugs": "https://github.com/b-vennes/nova-scala/issues",
  "categories": [
    "languages",
    "completions"
  ],
  "main": "main.js",
  "activationEvents": [
    "onLanguage:scala"
  ],
  "entitlements": {
    "process": true,
    "filesystem": "readwrite",
    "requests": true
  },
  "commands": {
    "command-palette": [
      {
        "title": "Metals: Import build",
        "command": "metals.importBuild",
        "filters": {
            "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Update",
        "command": "metals.update",
        "filters": {
            "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Doctor",
        "command": "metals.doctor",
        "fields": {
          "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Switch build server",
        "command": "metals.switchBuildServer",
        "fields": {
          "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Connect to build server",
        "command": "metals.connectToBuildServer",
        "fields": {
          "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Cancel compilation",
        "command": "metals.cancelCompilation",
        "fields": {
          "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Cascade compile",
        "command": "metals.cascadeCompile",
        "fields": {
          "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Clean compile",
        "command": "metals.cleanCompile",
        "fields": {
          "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Reset notifications",
        "command": "metals.resetNotifications",
        "fields": {
          "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Restart build server",
        "command": "metals.restartBuildServer",
        "fields": {
          "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Disconnect from old build server",
        "command": "metals.disconnectFromOldBuildServer",
        "fields": {
          "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: Reset workspace",
        "command": "metals.resetWorkspace",
        "fields": {
          "syntaxes": ["scala"]
        }
      },
      {
        "title": "Metals: List build targets",
        "command": "metals.listBuildTargets",
        "fields": {
          "syntaxes": ["scala"]
        }
      }
    ]
  }
}
